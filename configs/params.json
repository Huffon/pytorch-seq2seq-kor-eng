{
    "model": "seq2seq",
    "save_model": "model.pt",

    "mode": "train",
    "optim": "Adam",

    "random_seed": 32,
    "clip": 1,

    "learning_rate": 1e-3,
    "batch_size": 128,
    "num_epoch": 10,

    "embed_dim": 256,
    "hidden_dim": 512,
    "n_layer": 4,
    "dropout": 0.5,
    "bidirectional": true
}